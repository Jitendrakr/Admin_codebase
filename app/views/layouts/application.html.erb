  <meta name="description" content="">
  <meta name="author" content="">
  <title>Codebase</title>
  <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

  <%= stylesheet_link_tag    'application'%>
