        <div class="row">
          <div class="col-lg-12">
            <h3 class="page-header">header</h3>
          </div>
        </div>
        <div class="row">
          <%= yield %>
          <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->
  </div>
